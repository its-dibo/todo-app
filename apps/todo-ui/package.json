{
  "name": "todo-ui",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "nodemon",
    "build": "ng cache clean && ng build --configuration development",
    "postbuild": "npm run _node -- tasks/post-build.ts",
    "serve": "npm run _node -- dist/server/server.mjs dotenv_config_path=.env.development",
    "_node": "node -r tsconfig-paths/register -r dotenv/config --loader ts-node/esm --experimental-specifier-resolution=node --experimental-import-meta-resolve --no-warnings"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/cdk": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/material": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/platform-server": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@angular/service-worker": "^17.0.0",
    "@angular/ssr": "^17.0.0",
    "@ngx-formly/core": "^6.2.2",
    "@ngx-formly/material": "^6.2.2",
    "express": "^4.18.2",
    "http-proxy": "^1.18.1",
    "ngx-quill": "^24.0.4",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.0",
    "@types/express": "^4.17.21",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^20.10.4",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.2.2"
  }
}
